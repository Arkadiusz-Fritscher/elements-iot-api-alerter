{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"]
  },
  "include": [
    "./*.js",
    "src/**/*.ts",
    "test/**/*.ts", "backups/dataAccess.ts", "backups/dataAnalyze.ts", "backups/dataStorage.ts", "backups/utils.ts",
  ],
}
